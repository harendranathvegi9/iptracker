%tr{:id => cycle("odd", "even")}
	%td= link_to h(ip_address.ip_address), ip_address
	%td= h ip_address.name
	%td= h ip_address.lookup
	%td= h ip_address.reverse_lookup
	%td= link_to h(ip_address.cidr.name), ip_address.cidr
	%td= h ip_address.comments
	%td= h ip_address.free
	%td= h ip_address.ping
	%td= link_to 'Edit', edit_ip_address_path(ip_address)
